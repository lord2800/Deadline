{
	"/index": {
		"controller": "Home",
		"method": "start"
	},
	"/install": {
		"controller": "Install",
		"method": "webInstall"
	},
	"/admin": {
		"controller": "Admin",
		"method": "index"
	},
	"/admin/page/:?controller/:?method": {
		"controller": "Admin",
		"method": "page"
	},
	"/user/signin": {
		"controller": "Profile",
		"method": "signin"
	},
	"/user/signout": {
		"controller": "Profile",
		"method": "signout"
	},
	"/user/signup": {
		"controller": "Profile",
		"method": "signup"
	},
	"/user/:profile": {
		"controller": "Profile",
		"method": "edit"
	},
	"/role/:role": {
		"controller": "Profile",
		"method": "role"
	},
	"/blog/entry/:id": {
		"controller": "Blog",
		"method": "entry"
	},
	"/blog/edit/:?id": {
		"controller": "Blog",
		"method": "edit"
	},
	"/blog/publish/:id": {
		"controller": "Blog",
		"method": "publish"
	},
	"/blog/unpublish/:id": {
		"controller": "Blog",
		"method": "unpublish"
	},
	"/blog/delete/:id": {
		"controller": "Blog",
		"method": "delete"
	},
	"/blog/:?page": {
		"controller": "Blog",
		"method": "index"
	},
	"/video/list": {
		"controller": "Video",
		"method": "videos"
	},
	"/video/file": {
		"controller": "Video",
		"method": "file"
	},
	"/video/watch": {
		"controller": "Video",
		"method": "show"
	}
}